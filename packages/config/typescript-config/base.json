{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"incremental": false,
		"isolatedModules": true,
		"lib": [
			"es2022",
			"DOM",
			"DOM.Iterable"
		],
		"module": "NodeNext",
		"moduleDetection": "force",
		"noUncheckedIndexedAccess": true,
		"moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"preserveSymlinks": true,
		"strict": true,
		"target": "ES2022",
		"paths": {
			"@ui/custom/*": [
				"../../ui/custom/*"
			],
			"@ui/shadcn/*": [
				"../../ui/shadcn/*"
			],
			"@feature/editor/*": [
				"../../feature/editor/*"
			],
			"@feature/three/*": [
				"../../feature/three/*"
			],
			"@infra/trpc/*": [
				"../../infra/trpc/*"
			],
			"@infra/supabase/*": [
				"../../infra/supabase/src/*"
			],
			"@entity/memo/*": [
				"../../entity/memo/*"
			],
		}
	}
}
